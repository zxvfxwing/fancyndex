<h1> {{ total_size }} Bytes - {{ total_elts }} | Root: {{ root }} </h1>

{% if parent != "/" %}
<a href="{{ parent }}">Back</a>
{% endif %}

<h1> Directories </h1>
{% for d in directories %}
    <a href="{{ root }}/{{ d.name }}">{{ d.name }}</a><br>
{% endfor %}

<hr>
<h1> Files </h1>
{% for f in files %}
    <a href="{{ root }}/{{ f.name }}">{{ f.name }}</a><br>
{% endfor %}